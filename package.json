{
  "name": "e2e-playground",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines" : {
    "node" : ">=18.17.0"
  },
  "scripts": {
    "test-pw": "node_modules/.bin/playwright test",
